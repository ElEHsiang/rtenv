file main.elf
target remote :3333
set logging overwrite on
set logging file test-itoa.txt
set logging on
set cmd[0] = "0"
set itoa(0, cmd[0],10)
if !strcmp(cmd[0], "0")
    echo Pass: itoa correctly converts 0into "0"\n
else
    printf "Fail: itoa converts 0 into \"%s\", shall be \" 0\"\n", cmd[0]
end
set cmd[1]="-32768" 
set itoa(-32768, cmd[1], 10)
if !strcmp(cmd[1], "-32768")
    echo Pass: itoa correctly converts -32768 into "-32768"\n
else
    printf "Fail: itoa converts -32768 into \"%s\", shall be \"-32768\"\n"
end
set logging off
